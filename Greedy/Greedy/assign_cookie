class Solution {
    public int findContentChildren(int[] g, int[] s) {
        int greed =0;
        int cookie =0;

        Arrays.sort(g);
        Arrays.sort(s);
        while(cookie<s.length && greed<g.length){
            if(g[greed] <= s[cookie]){
                greed++;
            }
                cookie++;
        }
        return greed;
    }
}